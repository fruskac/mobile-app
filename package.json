    "react-native-i18n": "2.0.10",
    "react-native-permissions": "^1.1.1",
    "react-navigation": "1.0.0-beta.27",
    "redux-logger": "3.0.6",
    "redux-persist": "5.7.0",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1"
  },
  "devDependencies": {
    "babel-jest": "22.1.0",
    "babel-preset-react-native": "4.0.0",
